generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             Int      @id @default(autoincrement())
  email          String   @unique
  name           String
  surname        String
  direccionEnvio String
  role           RoleEnum
  arbolId        Int?
  password       String
}

enum RoleEnum {
  ADMIN
  USER
}
